{
  "name": "cart",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "node-sass": "4.13.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-scripts": "3.0.1",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "1.0.2",
    "reselect": "^4.0.0",
    "typesafe-actions": "4.3.0",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --collectCoverage",
    "eject": "react-scripts eject",
    "coverage": "open-cli ./coverage/lcov-report/index.html"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "3.9.2",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/react-redux": "7.0.9",
    "@types/redux-saga": "0.10.5",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.13.0",
    "enzyme-to-json": "3.3.5",
    "jest-enzyme": "7.0.2",
    "opn-cli": "5.0.0",
    "react-test-renderer": "16.8.6",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "**/*.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
